function checkRoomCover(callback) {
  var tip = '';
  var getRestTime = function (day) {
    var expTime,
        sec;
    day = day ? parseInt(day, 10) : 0;
    expTime = new Date();
    expTime.setDate(expTime.getDate() + day);
    expTime.setHours(24);
    expTime.setMinutes(0);
    expTime.setSeconds(0);
    sec = expTime.getTime() - new Date().getTime();
    return sec;
  };
  var coverstatus = $('#js_start_show').data('coverstatus');

  if (LocalStorage.get('nocovertip')) {
    callback(1);
    return;
  }

  if (coverstatus == -1) {
    tip = '请先上传封面再开播哦';
  } else if (coverstatus == 2) {
    tip = '新封面审核未通过，是否重新上传？';
  }

  if (coverstatus == -1 || coverstatus == 2) {
    $.dialog({
      content: tip,
      lock: true,
      width: 300,
      button: [{
        name: '去上传',
        callback: function () {
          location.href = '/room/my';
        }
      }, {
        name: '直接开播',
        callback: function () {
          callback();
        }
      }]
    });

  } else {
    callback(1)
  }


  LocalStorage.set('nocovertip', 1, getRestTime() / 1000);
}


function js_live_room(){
  if(typeof(is_room) !='undefined' && is_room == 1){
    room_live_room();
  }else{
    defaut_live_room();
  }
  $('#inter_tags').hide();
  $('#js_phone_div').hide();
}


$(function(){
  $('.r_close').click(function(){
      $('#js_phone_div').hide();
  });
});
