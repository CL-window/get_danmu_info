    <script type="text/javascript">

        var roomIsLiving = "True";

        $(function () {

            $("#btnPlay").click(function () {
                var boardCast_Title = $("#BoardCastTitle").val();

                if (boardCast_Title.length < 1) {
                    alert("请填写标题或视频流地址");
                    return false;
                }

                if (roomIsLiving == "False" && $(":radio[name='lines']").length>1) {
                    var lst_name = $(":checked[name='lines']").parent().text();
                    alert("您将选择线路“" + lst_name + "”开启直播，请确认您的串流工具里的直播地址和直播码是否正确");
                }

                $("#submitPlay").submit();
                return true;
            });

            $(":radio[name='lines']").on("click", function () {
                if (roomIsLiving == "True") { return; }
                var lst = $(this).val();

                $.get('GetUpStreamUrl?liveStreamType=' + lst, function (result) {
                    if (!result || !result.item1 || !result.item2) {
                        alert("切换线路失败");
                        return false;
                    }

                    $("#up_stream_host").val(result.item1);
                    $("#up_stream_key").val(result.item2);
                    $("#liveStreamType").val(lst);
                });
            });

        });

    </script>
