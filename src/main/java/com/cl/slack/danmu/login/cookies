<script type="text/javascript">
$(function(){
    $(".js_u_nav a").click(function(){
        if(!$(this).siblings('dl').is(":animated")){

            if( !$(this).find('b').attr('class') ){
                $(this).find('b').addClass('cur');
            }else{
                $(this).find('b').removeClass('cur');
            }
                $(this).siblings('dl').slideToggle();
        }
    });
})
</script>
