    var update_cate = {
        tag_id: cateId,
		id: childId,
        dialog: null,
        dialog_ld: null,
        loading: {
            ld: null,
            show: function () {
                update_cate.loading.ld = $.dialog({title: '正在提交中…', lock: true});
            },
            hide: function () {
                if (update_cate.loading.ld)
                    update_cate.loading.ld.close();
            }
        },
        show: function () {
            var content = '',
                name = {
                    total:childCount
                };
                sendData = {
                    tag_id: cateId,
                    id: childId
                };
            var url = '/CateList/getColumnList';
             $.ajax({
                 url: url,
                 type: 'GET',
                 dataType: 'html',
                 async: false,
                 data: {tagId: cateId, relate: 1, showVertical: 0},
                 success: function (data) {
                     content = data;
                 }
             })
             update_cate.dialog = $.dialog({
                 title: '<span style="float: right;color: #000;font-size: 14px;margin-right: 10px;">如需直播体育相关内容，请前往<a href="http://live.qq.com/" target="_blank" style="color: #3D3DEE;text-decoration:underline">企鹅直播</a></span>直播分类',
                 content: content,
                 fixed: true,
                 width: '720px',
                 top: '200px',
                 lock: true
             });
			pager.create({
				warp: '#change_tags',
				limit: 60,
				offset: 0,
				step: 5,
                showVertical: 0,
				success: function(data, index, param) {
					var $trigger = $(param.trigger),
						total = $trigger.data('total'),
						index = $trigger.closest('.tag_item').data('index');

					if (total <= 1 && index > 0) {
						param.contentBox.find('.tag_trigger').trigger('click');
					}
                    if(index == 0){
                        $("#tag_item_1 ul li a[data-id='"+ update_cate.tag_id +"']").addClass('tag_check');
                    }
                    if( index== 1 ){
                        $("#tag_item_2 ul li a[data-id='"+ update_cate.id +"']").addClass('tag_check');
                    }
				},
				callback: function(param) {
					$(this.config.warp).on('click', '#tag_back', function() {
						$('#tag_item_0, #tag_item_1').show();
						$('#tag_item_2').hide();
					});

					var $trigger = $(param.trigger),
						$parent = $trigger.closest('.tag_item'),
						total = $trigger.data('total'),
                        index = $parent.data('index');

					if ($trigger.hasClass('tag_trigger')) {
						$trigger.addClass('tag_check').parent().siblings().find('.tag_trigger').removeClass('tag_check');

						if(index == 1){
                            name.tag_name = $trigger.text();
                            name.child_name = "";
                            name.total = total;
                        }
                        if( index == 2 ){
                            name.child_name = $trigger.text() || "";
                        }
						if (total <= 1) {
							if (total < 1) {
								var pid = $trigger.data('pid'),
									id = $trigger.data('id'),
									short = $trigger.data('short'),
									data = {};

								if (pid) {
									data = {
										tag_id: pid,
										id: id
									};
								} else {
									data = {
										tag_id: id,
                                        id: 0,
										short: short
									};
								}

								update_cate.submit(data, name);
                                return;
							}

						}

						switch ( $parent.data('index') ) {
							case 1:
								$('#tag_item_0, #tag_item_1').hide();
								$('#tag_item_2').show();
								break;
							case 0:
							case 2:
								$('#tag_item_0, #tag_item_1').show();
								$('#tag_item_2').hide();
								break;
						}
					}
				}
			});

        },
        submit: function (data, name) {
            if (update_cate.dialog) {
                update_cate.dialog.close();
            }
            update_cate.loading.show();

			submitData = $.extend(true, data, {
                'ctn': get_cval()
			});

            $.post('/room/my/update_room_cate', submitData, function (result) {
                if(result.error == 0){
                    update_cate.loading.hide();
                    later_tip();
                    update_cate.tag_id = data.tag_id;
                    update_cate.id = data.id;
                    var msg = name.tag_name;
                    if( name.total>1){
                        if(name.child_name != ''){
                            msg = msg + '——' + name.child_name;
                        }
                    }
                    $("#tag_show").html('<li><a href="javascript:void(0);" class="tag_check01">' + msg + '</a></li>');
                    setTimeout(function () {
                        window.location.reload(true);
                    }, 3000);

                }else{
                    update_cate.loading.hide();
                    $.dialog.tips_black('<font color="#FFF">房间信息修改失败，请重试！</font>', 3);
                }
            });
        }
    };
$(document).ready(function(){
    $(".js_tipsy").bind("mousemove mouseleave", function(event){
        if(event.type == 'mouseleave')
        {
            $('#tipsy').hide();
        }
        else
        {
            var tpos = $(this).offset();
            $('#tipsy').css('top',tpos.top + $(this).outerHeight(true) + 6);

            var modify_left = ($('#tipsy').outerWidth(true) - $(this).outerWidth(true)) / 2;
            $('#tipsy').css('left',tpos.left - modify_left);

            $("#tiptit").html($(this).data("title"));
            $('#tipsy').show();
        }
    });

