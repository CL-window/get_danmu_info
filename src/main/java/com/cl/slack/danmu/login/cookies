var roomTagList = ["6"];
var roomId = 1721834;
$("#updateTag").click(function(){

    var tag_list = all_str  = '';
    for(var index in allTagList){
        if(roomTagList!= null && $.inArray(index, roomTagList) > -1){
            tag_list += '<a href="javascript:;" data-tid='+index+' class="cur" data-name='+htmlspecialchars(allTagList[index]['name'])+'><i>'+allTagList[index]['name']+'</i></a>';
        }else{
            //所有标签
            tag_list += '<a href="javascript:;" data-tid='+index+'  data-name='+htmlspecialchars(allTagList[index]['name'])+'><i>'+allTagList[index]['name']+'</i></a>';
        }
    }
    all_str += '<div class="aui_content" style="padding: 15px; width:510px;">'+
                    '<div class="add_tag clearfix">'+
                        '<div class="add_tli js_add_accept clearfix" style="margin-left:7px">直播过程中若发现标签和直播内容不相符的情况，管理员会调整您房间的标签，请根据自身填写合适的标签</div>'+
                        '<div class="add_tli js_add_accept clearfix">'+
                            '<p class="type fl">房间标签：</p>'+
                            '<div style="color:red">最多选择5个</div>'+
                        '</div>'+
                        '<div class="add_tli js_add_attack clearfix">'+
                            '<div class="js_add_bt add_label fl">'+tag_list+ '</div>'+
                        '</div>'+
                    '</div>'+
                '</div>';
    $.dialog({
        title: '直播标签设置',
        content: all_str,
        okVal: '确定修改分类',
        init: function () {
            addRoomTag();
        },
        ok: function () {
            var tag_id = '';
            if($('.js_add_bt').find('a.cur').length >0){
                $.each($('.js_add_bt').find('a.cur'),function(i,obj){
                    //当前选择的标签
                    tag_id += $(obj).data('tid') + '|';
                });
            }

            $.dialog.confirm('确定要修改标签', function () {
                $.post("/room/my/changeRoomTags", {room_id: roomId,"tag_id":tag_id,'ctn': get_cval()}, function (data) {
                        data = eval("("+data+")");
                        if(data['error']>0){
                            $.dialog.alert("更新失败："+data['msg']);
                        }else{
                            $.dialog.alert("操作成功，页面即将刷新.", function () {
                                window.location.href = '/room/my' ;
                            });
                        }
                });
            });
        },
        cancelVal: '取消',
        cancel: function () {
        },
        lock:true
    });
    return false;
});

/*
 *  房间标签二期优化
 *  by cuixiaogui@douyu.tv
 */
function addRoomTag() {
  var curLength = $('.js_add_attack a.cur').length;
  var MaxLength = 5;
  $('.js_add_attack').on('click', 'a', function() {
    if ($(this).hasClass('cur')) {
      $(this).removeClass('cur');
      curLength--;
    } else {
      if (curLength < MaxLength) {
        $(this).addClass('cur');
        curLength++;
      }else{
        $.dialog.alert("标签数目最多只能选择五个");
      }
    }
  });
}



/**
 * js 实现php htmlspecialchars 方法
 * @param {type} string
 * @param {type} quote_style
 * @param {type} charset
 * @param {type} double_encode
 * @returns {unresolved} */
function htmlspecialchars(string, quote_style, charset, double_encode) {

    var optTemp = 0,
      i = 0,
      noquotes = false;
    if (typeof quote_style === 'undefined' || quote_style === null) {
      quote_style = 2;
    }
    string = string.toString();
    if (double_encode !== false) { // Put this first to avoid double-encoding
      string = string.replace(/&/g, '&amp;');
    }
    string = string.replace(/</g, '&lt;')
      .replace(/>/g, '&gt;');

    var OPTS = {
      'ENT_NOQUOTES': 0,
      'ENT_HTML_QUOTE_SINGLE': 1,
      'ENT_HTML_QUOTE_DOUBLE': 2,
      'ENT_COMPAT': 2,
      'ENT_QUOTES': 3,
      'ENT_IGNORE': 4
    };
    if (quote_style === 0) {
      noquotes = true;
    }
    if (typeof quote_style !== 'number') { // Allow for a single string or an array of string flags
      quote_style = [].concat(quote_style);
      for (i = 0; i < quote_style.length; i++) {
        // Resolve string input to bitwise e.g. 'ENT_IGNORE' becomes 4
        if (OPTS[quote_style[i]] === 0) {
          noquotes = true;
        } else if (OPTS[quote_style[i]]) {
          optTemp = optTemp | OPTS[quote_style[i]];
        }
      }
      quote_style = optTemp;
    }
    if (quote_style & OPTS.ENT_HTML_QUOTE_SINGLE) {
      string = string.replace(/'/g, '&#039;');
    }
    if (!noquotes) {
      string = string.replace(/"/g, '&quot;');
    }
    return string;
}


$(function () {
    function initUpdateCityEvent() {
        //初始化城市选择
        var container = $('.select-city-container'),
            $container = $('.select-city-container'),
            province = $container.data('province') ? $container.data('province') : '',
            city = $container.data('city') ? $container.data('city') : '';
        var update_city = $('#update_city');
        this.selectProv = new SelectProv();
        this.selectProv.init({
            container: container,
            defaultProvince: province,
            defaultCity: city
        });

        update_city.on('click',function (argument) {
            $.dialog.confirm('确定要修改城市', function () {
                var addr = $('.select-city-container .city-input .city-title').html().trim();
                var addr_info = addr.split("/");
                var province = '', city = '';
                if (addr_info.length >= 2) {
                    province = addr_info[0].trim();
                    city = addr_info[1].trim();
                }
                $.post("/room/my/update_city", {
                    province: province,
                    city: city,
                    'ctn': get_cval()
                }, function (ret) {
                    if (ret.code == 1) {
                        $.dialog.tips_black(ret.msg);
                        window.location.reload();
                    } else {
                        $.dialog.tips_black(ret.msg);
                    }
                }, 'json');
            });
        });
    }
    initUpdateCityEvent();
});
