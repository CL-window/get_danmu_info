    var close_live_room = function(data){
        var reloadUrl = location.href.split('?')[0]+'?'+ new Date().getTime();
        $.dialog({
            content: '直播已经关闭!<br /><span style=" display:block; width : 260px; color:red;font-size: 10px;font-weight:700;">请再次确认您已经关闭了OBS等推流软件！如推流软件没有关闭，仍然可在自己的直播间看到画面。</span>',
            okVal: '确定',
            ok : function(){ },
            close: function(){
                liveEndFeedBack.show(data);
            }
        });
    };



    $("#js_close_show").click(function(){
        confirm_msg('您确定要关闭直播吗？<br /><span style="color:red;font-size: 10px;font-weight:700;">请先关闭OBS等推流软件再关闭直播</span>',
            function(){
                //loading.show();
                $.ajax({
                    url : '/room/my/close_show',
                    type : 'POST',
                    dataType : 'json',
                    data : {
                        ajax: 1, 'ctn': get_cval()
                    },
                    success : function(data){
                        if((data.error-0) === 0){
                            close_live_room(data);
                        } else{
                            alert(data.msg);
                        }
                    }
                })
            });
    });

    $("#js_start_show,#js_one_show").click(function(){
        var $el = $(this);

        if($el.data('doing') == 1) { return false; }

        $el.data('doing', 1);

        $.getJSON('/member/cp/get_phone_status',function(data){
            $el.data('doing', 0);

            if(data.result == -1){
                $.dialog.alert("您还没有验证手机号，不能开启直播");
                return;
            }else if(data.result == 2){
                // ident_status = data.error.ident_status;
                // mobile_phone = data.error.mobile_phone;
                // phonestatus  = data.error.phonestatus ;
                // $('#js_reg_captcha').click();
                // $('#phonenum_captcha').val('');
                if(data.error.ident_status == 1){
                    $('#js_ident_name').html('正在审核中，请审核通过后再来开播……');
                    $('#js_ident_button').html('确定');
                }
                $('#js_phone_div').show();
                return;
            }else{
                js_live_room();
            }
        });
    });


	$("#s_cata").on("blur",function(e){
		if((specific_str!=$("#s_cata").val())){

			var str=$("#s_cata").val();

            if(!/^[a-z]{1}[a-z0-9]{2,11}$/.test(str.toLowerCase())){
                $.dialog.tips_black('个性域名只能是全英文或英文+数字,限制3-12个字符');
                setTimeout(function(){$("#s_cata").val(specific_str)}, 1000);
                return false;
            }
            $.dialog.confirm('确认修改个性域名么，下次修改需等待30天，确定?',function(){
                $.ajax({
                    type: "POST",
                    url: "/room/my/update_room_specific",
                    data: {'s_cata': str, 'ctn': get_cval()},
                    dataType: "json",
                    success: function (json) {
                        if (json.error != 0) {
                            $("#s_cata").val(specific_str);
                            $.dialog.tips_black("<span style='color: red;'>修改失败：</span>"+json.msg);
                        } else {
                            $("#s_cata").val(str);
                            specific_str = str;
                            later_tip();
                        }
                    }
                });
            });
		}
	});

	$("#change_send_config").click(function(){
		var $this = $('#change_send_config');
		$this.data('doing', 1);
		$.dialog({
			title: '切换推流线路',
                                lock:true,
			content: '请选择线路：<select id="js_send_select"><option value="23">全网推流线路一A(空闲)</option><option value="21">全网推流线路一B(繁忙)</option><option value="20">全网推流线路三B(较繁忙)</option><option value="22">全网推流线路三A(较空闲)</option></select>',
			init: function(){
				$("#js_send_select").val(room_rtmp);
			},
			ok: function(){
				var rtmp_id = $("#js_send_select").val();
				if(rtmp_id != room_rtmp)
				{
					loading.show();
					$.post('/room/my/update_room_send', { 'rtmp_id': rtmp_id, 'ctn': get_cval() }, function(){
						room_rtmp = rtmp_id;
						loading.close();
						alert_msg('修改成功！下次开播时生效，如正在直播需关闭后再开播。', '', 4);
					});
				}
			}
		});
		return false;
	});

    var originalIndex = $("#specific_status")[0].selectedIndex;
    $("#specific_status").on("change",function(e){
        loading.show();
        var status = $(this).val();
        $.post('/room/my/update_specific_status',{status:status,'ctn': get_cval()},function(data){
            loading.close();
            data = eval("("+data+")");
            if(data.error==0){
                $.dialog.tips_black('已'+$("#specific_status option:selected").text()+'个性域名，请等待缓存更新');
            }else{
                $.dialog.tips_black('更新个性域名状态失败：<span style="color: red">'+data.msg+"</span>");
                setTimeout(function(){$("#specific_status ")[0].selectedIndex=originalIndex}, 1000); // 重置为原option
            }
        });
    });

	$("#js_live_notice").click(function(event) {
//		if(0 < 3000000)
//		{
//			alert_msg('开播预告功能需体重大于 3T 才能使用');
//			return false;
//		}
		if(room_notice !== null && room_notice.show_time > Math.round(new Date().getTime()/1000)) {
			$.dialog({
				title: '房间预告',
				content: '你已添加了开播时间为 ' + timetodate_all(room_notice.show_time) + ' 的预告，确定要取消吗？',
				ok: function(){
					loading.show();
					$.post('/room/my/room_notice_del', {'ctn': get_cval(), id: room_notice.id }, function(data) {
						loading.close();
						room_notice = null;
						$.dialog.tips_black('房间预告已取消');
					});
				},
				cancelVal: '关闭',
				cancel: true
			});
		} else {
			$.dialog({
				title: '添加房间预告',
				id: 'live_notice_add',
				content: document.getElementById('js_live_notice_form'),
                lock:true,
				ok: function(){
					var title = $("#js_live_title").val();
					title = $.trim(title);
					if (title == '') {
						alert('标题不能为空');
						return false;
					}

                                            if (title.length> 15) {
						alert('标题不能超过15个字');
						return false;
					}

					var show_time = $("#js_live_time").val();
					if (show_time == '') {
						alert('开播时间不能为空');
						return false;
					}

					loading.show();
					$.post('/room/my/room_notice', {'ctn': get_cval(),
							title: title, show_time: show_time
						}, function(data) {
						loading.close();
						if (data.error == 0) {
							room_notice = data.msg;
							$.dialog.tips_black('房间预告添加成功');
						} else {
							alert_msg(data.msg);
						}
					}, 'json');

				},
				cancel: true
			});
		}
		return false;
	});
});


var pwd_str='';
function change_pwd()
{
	$.dialog.prompt('请输入房间密码：', function(str){
		str = $.trim(str);
		if(str){
			var reg = /^([0-9]\d*)$/;
			if(!reg.test(str))
			{
				$.dialog.tips_black('请输入整数');
				return false;
			}
		}

		loading.show();
		$.ajax({
			type: "POST",
			url: "/room/my/change_pwd",
			data:{'pwd' : str, 'ctn': get_cval()},
			dataType: "json",
			success:function(json){
				loading.close();
				if(json.ret!= 0){
					$.dialog.tips_black(json.msg);
				}else{
					pwd_str=str;
					$.dialog.tips_black('设置成功');
					//$("#chagepwd").html(str ? '修改房间密码' : '设置房间密码');
				}
			}
		});

	}, pwd_str);
}

function upload_age_limit(){
	$.dialog({
		id: 'setagelimit',
		title: '用户发言设置',
		content:document.getElementById('js_speak_set'),
		ok: function () {
			var str=$("#s_agelimit").val();
			str = $.trim(str);
			$("#s_agelimit").val(str);

			var reg=/^\d*$/;
			if(!reg.test(str)){
				$.dialog.tips_black('最低注册时间填写有误');
				return false;
			}


			var speak_cd=$("#speak_cd").val();
			speak_cd = $.trim(speak_cd);
			$("#speak_cd").val(speak_cd);

			if(!reg.test(speak_cd)){
				$.dialog.tips_black('CD发言倍数填写有误');
				return false;
			}
			speak_cd = parseInt(speak_cd, 10);
			if(speak_cd < 1 || speak_cd > 100){
				$.dialog.tips_black('CD发言倍数只能在1~100之间');
				return false;
			}

			loading.show();
			$.ajax({
				type: "POST",
				url: "/room/my/speak_set",
				data:$("#js_speak_set").serialize() + '&ctn=' + get_cval(),
				success:function(){
					loading.close();
					$.dialog.tips_black('提交成功！');
				}
			});
		},
		cancelVal: '关闭',
        lock:true,
		cancel: true
	});

}

var editor;

function update_show_details(obj){
    if($(obj).data('locked') == true){
        $.dialog.tips_black('您暂时不能修改房间公告！');
        return false;
    }
	if($(obj).data('update') == 'yes')
	{
		if($(obj).data('doing') == 'yes')
			return false;

		var str=$("#set_show_details").val();
		$(obj).html('保存中…').data('doing', 'yes');
		$.ajax({
			type: "POST",
			url: '/room/my/update_room_show_details',
			data:{'show_detail' : str, 'ctn': get_cval()},
			dataType: "json",
			success:function(json){
				if(json.ret!= 0){
					$.dialog.tips_black(json.msg);
                    $(obj).html('修改').removeData('doing update');
                    $("#show_details").html('');
				}else{
					$(obj).html('修改').removeData('doing update');
					$("#show_details").html(str);
					later_tip();
				}
			}
		});
	} else {
		var show_details = $("#show_details").html();
		$("#show_details").html('<textarea id="set_show_details" style="width:640px;height:80px;" maxlength="128">'+show_details+'</textarea>');
		$(obj).html('保存').data('update', 'yes');
	}
}

function later_tip()
{
	$.dialog.tips_black('<font color="#FFF">房间信息修改成功，需要1~2分钟更新，请耐心等待</font>', 3);
}

function getByteLen(val) {
	var len = val.length;
	if(val.match(/[^\x00-\xff]/ig) != null)
	{
		var alen = val.match(/[^\x00-\xff]/ig).length;
		len = len + alen*2
	}

	return len;
}

function get_cval()
{
	return get_cookie('ccn');
}

function room_live_room(){
    var $this = $("#js_start_show");
    var nocovertip = 'nocovertip';
    var param = {};
    var getRestTime = function (day) {
        var expTime,
            sec;
        day = day ? parseInt(day, 10) : 0;
        expTime = new Date();
        expTime.setDate(expTime.getDate() + day);
        expTime.setHours(24);
        expTime.setMinutes(0);
        expTime.setSeconds(0);
        sec = expTime.getTime() - new Date().getTime();
        return sec;
    };

    $(this).data('doing', 1);
    loading.lock = true;
    loading.show('正在提交，请勿刷新页面……');
    var csrf_cookie_name = "ctn";
    param[csrf_cookie_name] = get_cval();
    if (LocalStorage.get(nocovertip)) param['notshowtip'] = 1; // 是否不需要提示
    $.post('/room/my/first_show', param,
        function(back_data){
            var tip_icon = 'warning';
            loading.close();
            $this.data('doing', 0);
            if (back_data.error == 3) {
                $.dialog.alert("异常关闭<br />原因：<b>" + back_data.msg.reason + "</b><br />剩余限时：<span style='color: red'>" + back_data.msg.open_time + "</span>");
                return;
            } else if (back_data.error == 12 || back_data.error == 13) {
                if (LocalStorage.get(nocovertip)) {
                    tip_icon = 'succeed';
                    showstartLiveError(tip_icon, back_data);
                } else {
                    $.dialog({
                        content: back_data.msg,
                        lock: true,
                        width: 300,
                        button: [{
                            name: '去上传',
                            callback: function () {
                                location.href = '/room/my';
                            }
                        }, {
                            name: '直接开播',
                            callback: function () {
                                var csrf_cookie_name = "ctn";
                                param[csrf_cookie_name] = get_cval();
                                param['notshowtip'] = 1;
                                $.post('/room/my/first_show', param).then(function () {
                                    window.location.href = '/1721834?_r=' + Math.random();
                                });
                            }
                        }]
                    });
                }

                LocalStorage.set(nocovertip, 1, getRestTime() / 1000);
                return;
            } else if (back_data.error == 0) {
                $this.addClass('switch_on');
                tip_icon = 'succeed';
                showstartLiveError(tip_icon, back_data);
                return;
            } if(back_data.error == 9){
                locationVerify.init(back_data.msg.phone,back_data.msg);
                return;
            }

            showstartLiveError(tip_icon, back_data);
        }, 'json');
}

function showstartLiveError(tip_icon, back_data) {
    $.dialog({
        icon: tip_icon,
        content: back_data.msg,
        lock: true,
        okVal: '确定',
        ok: function () {
            //succeed 则刷新页面  否则按照要求来刷新页面
            if (tip_icon == 'succeed')
                window.location.href = '/1721834?_r=' + Math.random(1);
            else {
                if (back_data.error == 10) {
                    window.location.href = 'http://mail.qq.com';
                } else if (back_data.error == 1) {
                    window.location.href = '/room/my';
                } else if(back_data.error == 11 || back_data.error == 2) {
                    window.location.href = '/member/cp/changeIdent';
                }
            }
            return true;
        }
    });
}

